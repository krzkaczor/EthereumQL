  "scripts": {
    "start": "ts-node lib/index.ts",
    "test": "yarn format && yarn tsc && yarn test:mocha",
    "test:fix": "yarn format:fix && yarn tsc && yarn test:mocha",
    "tsc": "tsc --noEmit",
    "format": "prettier --list-different '**/*.ts'",
    "format:fix": "prettier --write '**/*.ts'",
    "test:mocha": "NODE_ENV=test mocha --require ts-node/register --require ./test/setupTests.ts 'lib/**/*.spec.ts' 'test/**/*.spec.ts'"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.1",
    "@types/chai": "^4.1.2",
    "@types/graphql": "^0.13.1",
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "ganache-cli": "^6.1.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "mocha": "^5.0.5",
    "prettier": "^1.11.1",
    "snap-shot-it": "^4.1.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.8.1",
    "typestrict": "^0.0.8",
    "web3-typescript-typings": "^0.10.2"
  },
  "dependencies": {
