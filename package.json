{
  "name": "EthereumQL",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node lib/index.ts",
    "test": "yarn format && yarn tsc && yarn test:mocha",
    "test:fix": "yarn format:fix && yarn tsc && yarn test:mocha",
    "tsc": "tsc --noEmit",
    "format": "prettier --list-different **/*.ts",
    "format:fix": "prettier --write **/*.ts",
    "test:mocha": "NODE_ENV=test mocha --require ts-node/register --require ./test/setupTests.ts './test/setupTestsHooks.ts' 'lib/**/*.spec.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "prettier": "^1.11.1",
    "snap-shot-it": "^4.1.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.8.1",
    "typestrict": "^0.0.8"
  },
  "dependencies": {
    "inversify": "^4.11.1",
    "reflect-metadata": "^0.1.12",
    "lodash": "^4.17.5"
  }
}
